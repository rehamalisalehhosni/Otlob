<p id="notice"><%= notice %></p>

<h1>Listing Invited Fiends</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invited_fiends.each do |invited_fiend| %>
      <tr>
        <td><%= invited_fiend.status %></td>
        <td><%= link_to 'Show', invited_fiend %></td>
        <td><%= link_to 'Edit', edit_invited_fiend_path(invited_fiend) %></td>
        <td><%= link_to 'Destroy', invited_fiend, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Invited fiend', new_invited_fiend_path %>
